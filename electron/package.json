{
  "name": "electron",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "src/main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron:build": "electron-builder",
    "preview": "vite preview"
  },
  "build": {
    "appId": "com.example.vue-electron",
    "productName": "VueElectronPinia",
    "asar": true,
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "src/main/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "deb"
    }
  },
  "dependencies": {
    "axios": "^1.13.2",
    "echarts": "^5.6.0",
    "vue": "^3.5.24",
    "vue-echarts": "^6.7.3",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "vite": "^7.2.4",
    "vite-plugin-electron": "^0.29.0"
  }
}
